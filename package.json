{
  "name": "storefrontapi",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start":"npm run build && node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "npx tsc",
    "db:up": "db-migrate up",
    "db:down": "db-migrate reset",
    "db:up-dev": "db-migrate up --env test",
    "db:down-dev": "db-migrate reset --env test",
    "db:seed": "npm run build&&node dist/utils/seeders/db.seeder.js",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "eslint": "eslint . --ext .ts",
    "test": "npm run db:down-dev&&npm run build&&set Node_ENV=test&&npm run db:up-dev&&node dist/utils/seeders/db.seeder.js&&jasmine&&npm run db:down-dev",
    "jasmine": "jasmine"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.4",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "supertest": "^6.2.4"
  }
}
